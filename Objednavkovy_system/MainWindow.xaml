<Window x:Class="Objednavkovy_system.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Objednavkovy_system"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button x:Name="show">Ukázat</Button>
        <StackPanel x:Name="menu">
            <Button x:Name="customers">Zákazníci</Button>
            <Button x:Name="games">Hry</Button>
        </StackPanel>
        <Frame x:Name="frame"></Frame>
    </StackPanel>
</Window>
